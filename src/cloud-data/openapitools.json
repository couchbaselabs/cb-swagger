{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.4.0",
    "generators": {
      "v3.0": {
        "generatorName": "asciidoc",
        "inputSpec": "#{cwd}/swagger/elixir-openapi.yaml",
        "output": "#{cwd}/../../docs/modules/data-api-reference/pages",
        "templateDir": "#{cwd}/../../templates",
        "additionalProperties": {
          "specDir": "#{cwd}/../../docs/modules/data-api-reference/partials/",
          "snippetDir": "#{cwd}/../../docs/modules/data-api-reference/partials/paths/",
          "headerAttributes": false,
          "useIntroduction": true,
          "useTableTitles": true,
          "legacyDiscriminatorBehavior": true
        },
        "globalProperty": {
          "generateAliasAsModel": true
        },
        "gitRepoId": "cb-swagger",
        "gitUserId": "couchbase"
      }
    }
  }
}
